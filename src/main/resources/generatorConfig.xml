<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <!-- 引入配置文件 -->
    <properties resource="generator.properties" />

    <context id="context1" targetRuntime="MyBatis3">

        <!-- 数据库连接信息 -->
        <jdbcConnection driverClass="${jdbc.driverClassName}"
                        connectionURL="${jdbc.url}" userId="${jdbc.username}" password="${jdbc.password}" />

        <!-- 生成Model对象路径配置 -->
        <javaModelGenerator targetPackage="org.saas.report.entity"
                            targetProject="src\main\java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <!-- 生成sqlXML文件路径配置 -->
        <sqlMapGenerator targetPackage="mapping"
                         targetProject="src\main\resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>

        <!-- 生成DAO的类文件路径配置 -->
        <javaClientGenerator targetPackage="org.saas.report.dao"
                             targetProject="src\main\java" type="XMLMAPPER">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>

        <!--要生成哪些表 -->
        <table tableName="S02_dec_stat_yr" domainObjectName="S02DecStatYr" />
        <table tableName="S02_dec_amt" domainObjectName="S02DecAmt" />
        <table tableName="S02_ie_dec_stat" domainObjectName="S02IeDecStat" />
        <table tableName="S02_entp_dec_qty" domainObjectName="S02EntpDecQty" />
        <table tableName="S02_nincr_cust_qty" domainObjectName="S02NincrCustQty" />
        <table tableName="S02_entp_cust_dec_qty" domainObjectName="S02EntpCustDecQty" />
    </context>
</generatorConfiguration>